{
  "extends": "airbnb-base",
  "rules": {
    "semi": ["error", "never"],
    "object-curly-spacing": ["error", "never"],
    "no-use-before-define": ["error", {"functions": false, "classes": true}],
    "no-underscore-dangle": 0,
    "no-console": ["error", { allow: ["warn", "error"], }],
    arrow-body-style: ["error", "always"]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
